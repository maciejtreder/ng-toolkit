{
  "name": "application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-p build:watch credentials",
    "build": "ng build",
    "test": "echo no-tests",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "deploy": "serverless deploy",
    "build:deploy:aws": "npm run build:prod && npm run deploy:aws",
    "build:deploy:gcloud": "npm run build:prod && npm run deploy:gcloud",
    "deploy:aws": "cp-cli serverless-aws.yml serverless.yml && npm run deploy",
    "deploy:gcloud": "cp-cli serverless-gcloud.yml serverless.yml && npm run deploy",
    "postinstall": "node postinstall.js",
    "build:watch": "ng serve",
    "credentials": "node credentials.js && opencollective postinstall",
    "build:dev": "run-p test:watch start",
    "build:prod": "npm run build:client-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "server": "node local.js",
    "build:watch:firebug": "ng serve -c firebug",
    "build:firebug": "run-p build:watch:firebug credentials",
    "build:client-and-server-bundles": "ng build --prod && ng run application:server",
    "test:watch": "ng test --watch --code-coverage"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.803.16",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@nguniversal/express-engine": "^8.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "ajv": "^6.5.4",
    "aws-serverless-express": "^3.2.0",
    "core-js": "^2.5.4",
    "cors": "~2.8.4",
    "firebase-admin": "^5.11.0",
    "firebase-functions": "^0.9.1",
    "rxjs": "^6.0.0",
    "typescript-collections": "^1.3.2",
    "webpack-cli": "3.3.9",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular/cli": "^8.0.8",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-google-cloudfunctions": "^1.1.1",
    "cp-cli": "^1.1.0",
    "cpy-cli": "^1.0.1",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "node-wget": "^0.4.2",
    "npm-run-all": "^4.1.2",
    "opencollective": "^1.0.3",
    "pre-commit": "^1.2.2",
    "reflect-metadata": "^0.1.10",
    "serverless": "1.26.1",
    "sinon": "^4.5.0",
    "ts-loader": "4.2.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.7.0",
    "typescript": "~3.4.5"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/angular-universal-pwa"
  }
}

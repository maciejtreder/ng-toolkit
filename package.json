{
  "author": "Maciej Treder <contact@maciejtreder.com>",
  "description": "Angular Universal boilerplate for serverless environment. Built on top of https://github.com/FrozenPandaz/ng-universal-demo",
  "name": "angular-universal-serverless",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maciejtreder/angular-universal-serverless.git"
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "test": "npm run lint && karma start --single-run",
    "test:dev": "karma start",
    "postinstall": "npm run build:dll",
    "start": "webpack-dev-server --env.client",
    "build:dev": "npm-run-all -p -r start test:dev",
    "build:dll": "rimraf dll && webpack --env.dll",
    "build:prod:client": "webpack --env.aot --env.client -p",
    "build:prod:server": "webpack --env.server",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
    "build:prod": "npm run build:prod:client && npm run service-worker && npm run build:prod:server && npm run ngsw-copy",
    "service-worker": "ngsw-config dist src/ngsw-manifest.json",
    "build:deploy": "npm run build:prod && npm run deploy",
    "sass:watch": "node-sass  --include-path node_modules --output-style compressed -q",
    "prebuild:prod": "rimraf dist",
    "server": "node local.js",
    "deploy": "serverless deploy"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@angular-universal-serverless/external-module": "git+https://github.com/maciejtreder/angular-external-module.git",
    "@angular/animations": "^5.0.0",
    "@angular/app-shell": "^0.1.0",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@angular/service-worker": "^5.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "aws-serverless-express": "3.0.0",
    "body-parser": "^1.17.2",
